{
  "name": "swe-brunch",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "nodemon -w .nodemonrestart --exec 'next dev'",
    "build": "next build; touch .nodemonrestart",
    "start": "next start",
    "_lint": "prettier --log-level error -w ./app ./components ./hooks ./stores ./utils",
    "prepare": "husky",
    "release": "commit-and-tag-version",
    "release:undo": "git tag -d $(git tag --sort=-creatordate | head -n 1); git reset --hard HEAD~1",
    "release:major": "commit-and-tag-version -r major"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.5",
    "clsx": "^2.1.1",
    "next": "16.0.1",
    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "commit-and-tag-version": "^12.6.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5"
  }
}
